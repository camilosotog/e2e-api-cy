{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["cypress/e2e/ui/pages/*"],
      "@data/*": ["cypress/e2e/ui/data/*"],
      "@helpers/*": ["cypress/e2e/ui/helpers/*"],
      "@interfaces/*": ["cypress/e2e/ui/interfaces/*"]
    }
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
